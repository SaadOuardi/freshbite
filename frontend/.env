# API Configurations
REACT_APP_API_BASE_URL=https://api.yourrestaurant.com
REACT_APP_IMAGE_BASE_URL=https://cdn.yourrestaurant.com/images

# Stripe or Payment Gateway Keys
REACT_APP_STRIPE_PUBLIC_KEY=pk_test_your_stripe_public_key
# Add any other payment service public keys if needed

# Firebase Configurations (if used)
REACT_APP_FIREBASE_API_KEY=your_firebase_api_key
REACT_APP_FIREBASE_AUTH_DOMAIN=your_project_id.firebaseapp.com
REACT_APP_FIREBASE_PROJECT_ID=your_project_id
REACT_APP_FIREBASE_STORAGE_BUCKET=your_project_id.appspot.com
REACT_APP_FIREBASE_MESSAGING_SENDER_ID=your_messaging_sender_id
REACT_APP_FIREBASE_APP_ID=your_app_id

# Website Metadata
REACT_APP_WEBSITE_NAME=FreshBite
REACT_APP_WEBSITE_URL=https://saadouardi.github.io/freshbite
REACT_APP_WEBSITE_CONTACT_EMAIL=contact@freshbite.com
REACT_APP_WEBSITE_PHONE_NUMBER=+1234567890
REACT_APP_WEBSITE_CURRENCY=USD

# Map API for Restaurant Location
REACT_APP_MAP_API_KEY=your_google_map_api_key

# Environment Mode
REACT_APP_ENV=production

# Social Media Links
REACT_APP_FACEBOOK_URL=https://facebook.com/freshbite
REACT_APP_INSTAGRAM_URL=https://instagram.com/freshbite
REACT_APP_TWITTER_URL=https://twitter.com/freshbite

# Miscellaneous
REACT_APP_DEFAULT_LANGUAGE=en
REACT_APP_SUPPORT_EMAIL=support@freshbite.com
